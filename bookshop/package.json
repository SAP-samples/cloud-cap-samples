{
  "name": "@capire/bookshop",
  "version": "1.0.0",
  "description": "A simple self-contained bookshop service.",
  "type": "module",
  "files": [
    "app",
    "srv",
    "db",
    "index.cds",
    "index.js"
  ],
  "dependencies": {
    "@sap/cds": ">=5.9",
    "express": "^4.17.1",
    "passport": ">=0.4.1"
  },
  "scripts": {
    "genres": "cds serve test/genres.cds",
    "start": "cds run",
    "watch": "cds watch"
  },
  "cds": {
    "requires": {
      "db": "sql",
      "[mtx]": {
        "multitenancy": "saas-deployment",
        "[sidecar]": {
          "deployment-service": "from-sidecar",
          "model-provider": "from-sidecar",
          "db": "sqlite"
        }
      }
    }
  }
}
